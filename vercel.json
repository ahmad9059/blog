{
  "build": {
    "env": {
      "HUGO_VERSION": "0.140.2",
      "GO_VERSION": "1.20.6"
    }
  },
  "buildCommand": "hugo --gc --minify",
  "rewrites": [
    { "source": "/install", "destination": "/api/install.js" },
    { "source": "/complete", "destination": "/api/complete.js" },
    { "source": "/install-auto", "destination": "/api/install-auto.js" }
  ]
}
